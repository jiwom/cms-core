{
  "name": "yajra/cms-core",
  "description": "Core module of YajraCMS.",
  "keywords": [
    "yajracms",
    "laravel",
    "core",
    "module"
  ],
  "homepage": "https://github.com/yajra/cms-core",
  "license": "MIT",
  "authors": [
    {
      "name": "Arjay Angeles",
      "email": "aqangeles@gmail.com",
      "homepage": "http://yajrabox.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "yajra/cms-themes": "^2.0",
    "yajra/laravel-datatables-oracle": "7.*",
    "yajra/laravel-datatables-buttons": "1.*",
    "yajra/laravel-acl": "^3.0.0",
    "yajra/laravel-auditable": "^1.0",
    "baum/baum": "^1.1",
    "laracasts/flash": "^1.3",
    "laracasts/presenter": "^0.2.1",
    "barryvdh/laravel-snappy": "^0.3",
    "barryvdh/laravel-debugbar": "^2.2",
    "spatie/laravel-fractal": "^3.0",
    "spatie/laravel-backup": "^3.7",
    "spatie/laravel-sluggable": "^1.1",
    "spatie/eloquent-sortable": "^2.1",
    "caffeinated/menus": "^3.0",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "guzzlehttp/guzzle": "^6.1",
    "arrilot/laravel-widgets": "^3.4",
    "intervention/image": "^2.3",
    "rap2hpoutre/laravel-log-viewer": "^0.6.1",
    "themsaid/laravel-mail-preview": "^1.1",
    "rtconner/laravel-tagging": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "Yajra\\CMS\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Yajra\\CMS\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
